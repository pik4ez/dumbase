#: ./dumbase.py:24
msgid "Dumping database like a boss."
msgstr "Дампит базы как заводной."

#: ./dumbase.py:30
msgid "lists tables from specified $dsn using blacklisting and whitelisting"
msgstr "перечисляет таблицы для указанной базы, учитывая вайт- и блэк-листы"

#: ./dumbase.py:36
msgid "data source name for da target database: [$user@[:password]]$host[:$port]/$dbname"
msgstr "параметры доступа к целевой базе: [$user@[:password]]$host[:$port]/$dbname"

#: ./dumbase.py:41
msgid "dumping data from $source_dsn into $target_dsn"
msgstr ""

#: ./dumbase.py:45
msgid "data source name for the source database (data will be imported from): [$user@[:password]]$host[:$port]/$dbname"
msgstr "параметры доступа к исходной базе (из которой будут браться данные): [$user@[:password]]$host[:$port]/$dbname"

#: ./dumbase.py:52
msgid "data source name for the target database (data will be exported to): [$user@[:password]]$host[:$port]/$dbname"
msgstr "параметры доступа к целевой базе (в которую будут загружаться данные): [$user@[:password]]$host[:$port]/$dbname"

#: ./dumbase.py:63
msgid "white table for dumping (regex)"
msgstr "включить таблицу в дамп (возможно использование регулярных выражений)"

#: ./dumbase.py:72
msgid "ignores table for dumping (regex)"
msgstr "исключить таблицу из дампа (возможно использование регулярных выражений)"

#: ./dumbase.py:78
msgid "sets to ignore all tables (use --white to specify tables you need)"
msgstr "исключает все таблицы из дампа (необходимо использовать ключ --white для включения таблиц в дамп по одной)"

#: ./dumbase.py:84
msgid "includes triggers into database dump"
msgstr "включает триггеры в дамп"

#: ./dumbase.py:103
msgid "failed to connect source database"
msgstr "ошибка при соединении с исходной базой"

#: ./dumbase.py:112
msgid "leave empty if same"
msgstr "оставьте пустым, если совпадает с паролем для исходной базы"

#: ./dumbase.py:120
msgid "failed to connect target database"
msgstr "ошибка при соединении с целевой базой"

#: ./dumbase.py:130
msgid "<no tables>"
msgstr "<нет таблиц>"

#: ./dumbase.py:136
msgid "if you use --black-all flag, you must specify at least one matching --white flag"
msgstr "при использовании ключа --black-all необходимо включить в дамп хотя бы одну таблицу с помощью ключа --white"

#: ./dumbase.py:150
msgid "Recent dump found in {0}. Should I use it? [Y/n]: "
msgstr "Актуальный дамп найден в файле {0}. Использовать его? [Y/n]: "

